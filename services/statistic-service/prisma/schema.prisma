// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model NFTDetail {
  nft_id          Int @id
  eth             Float
  relativeEth     Float
  rank            Int
  lastUpdate      String
  nft_owner_url   String?
  nft_owner_name  String?
  weeklyRank      Int
}
