generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model NFTDetail {
  nft_id         Int     @id
  eth            Float
  relativeEth    Float
  rank           Int
  lastUpdate     String
  nft_owner_url  String?
  nft_owner_name String?
  weeklyRank     Int
}

model DonatorInformation {
  donatedTokenId  Int       @id
  email           String
  dateOfListing   String 
}